{
  "manifest_version": 3,
  "name": "Code Analysis Assistant",
  "version": "1.0",
  "description": "AI-powered code analysis and recommendations",
  "permissions": ["activeTab", "storage", "nativeMessaging", "sidePanel", "tabs"],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
